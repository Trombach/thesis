  year = {2009},
  pages = {064306--064306},
  file = {Assadollahzadeh, Schwerdtfeger - 2009 - A systematic search for minimum structures of small gold clusters Au n (n=2-20) and their electr:/home/trombach/.zotero/zotero/yhjnkvs7.default/zotero/storage/54UTG7HB/Assadollahzadeh, Schwerdtfeger - 2009 - A systematic search for minimum structures of small gold clusters Au n (n=2-20) and their electr.pdf:application/pdf},
  groups = {gold fullerenes}
}

