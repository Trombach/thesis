  file = {Chen et al. - 2010 - Structures of neutral and anionic Au(16) clusters revisited:/home/trombach/.zotero/zotero/yhjnkvs7.default/zotero/storage/S8DW89QW/Chen et al. - 2010 - Structures of neutral and anionic Au(16) clusters revisited.pdf:application/pdf},
  groups = {gold fullerenes}
}

