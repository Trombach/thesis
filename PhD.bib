  journal = {Physical Chemistry Chemical Physics},
  author = {Autschbach, Jochen and a Hess, Bernd and Johansson, Mikael P and Neugebauer, Johannes and Patzschke, Michael and Pyykk\dbend{}, Pekka and Reiher, Markus and Sundholm, Dage},
