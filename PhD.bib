  author = {Verheijen, M. A. and Meekes, H. and Meijer, G. and Bennema, P. and {de Boer}, J. L. and {van Smaalen}, S. and {van Tendeloo}, G. and Amelinckx, S. and Muto, S. and {van Landuyt}, J.},
