@article{donev_unusually_2004,
  title = {Unusually {{Dense Crystal Packings}} of {{Ellipsoids}}},
  volume = {92},
  doi = {10.1103/PhysRevLett.92.255506},
  abstract = {In this Letter, we report on the densest-known packings of congruent ellipsoids. The family of new packings consists of crystal arrangements of spheroids with a wide range of aspect ratios, and with density $\varphi$ always surpassing that of the densest Bravais lattice packing $\varphi\approx$0.7405. A remarkable maximum density of $\phi\approx$0.7707 is achieved for maximal aspect ratios larger than $\surd$3, when each ellipsoid has 14 touching neighbors. Our results are directly relevant to understanding the equilibrium behavior of systems of hard ellipsoids, and, in particular, the solid and glassy phases.},
  timestamp = {2017-06-08T04:37:08Z},
  number = {25},
  journal = {Physical Review Letters},
  author = {Donev, Aleksandar and Stillinger, Frank H. and Chaikin, P. M. and Torquato, Salvatore},
  month = jun,
  year = {2004},
  pages = {255506},
