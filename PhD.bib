  file = {PhysRevB.59.1930.pdf:/home/trombach/.zotero/zotero/yhjnkvs7.default/zotero/storage/9VHZD2AN/PhysRevB.59.1930.pdf:application/pdf;APS Snapshot:/home/trombach/.zotero/zotero/yhjnkvs7.default/zotero/storage/UCGGVRK2/PhysRevB.59.html:text/html},
  groups = {fullerenes}
}

@article{okada_new_1999,
