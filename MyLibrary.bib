	url = {http://stacks.iop.org/0953-8984/25/i=43/a=435303},
	doi = {10.1088/0953-8984/25/43/435303},
	shorttitle = {Continuous transformations of C 60 crystals},
	abstract = {Application of pressure is a versatile method to tailor the properties of organic semiconductors. For example, it is known that high pressure can transform C 60 face-centred-cubic ({FCC}) crystals to polymer structures with inter-molecular bonds. Here we use first-principles calculations to describe continuous crystalline transformation paths that include the {FCC} and polymer structures as distinct local energy minima. In addition to analysing the atomic-scale details of polymerization, we obtain the ideal strength of {FCC}-C 60 , identify metastable C 60 crystalline polymorphs, and characterize their electronic properties—all key features for the performance of C 60 crystals in organic electronic devices.},
	pages = {435303},
	number = {43},
	journaltitle = {Journal of Physics: Condensed Matter},
	shortjournal = {J. Phys.: Condens. Matter},
	author = {Volonakis, G. and Tsetseris, L. and Logothetidis, S.},
