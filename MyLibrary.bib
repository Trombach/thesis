  file = {/home/trombach/.zotero/zotero/yhjnkvs7.default/zotero/storage/E3JXF2A3/Takahata, Okamoto, Chong - 2006 - DFT calculation of core-electron binding energies of pyrimidine and purine bases.pdf}
}

