@article{arkus_deriving_2011,
	title = {Deriving Finite Sphere Packings},
	volume = {25},
	issn = {0895-4801},
	url = {http://epubs.siam.org/doi/abs/10.1137/100784424},
	doi = {10.1137/100784424},
	abstract = {Sphere packing problems have a rich history in both mathematics and physics; yet, relatively few analytical analyses of sphere packings exist, and answers to seemingly simple questions are unknown. Here, we present an analytical method for deriving all packings of n spheres in \${\textbackslash}mathbb\{R\}{\textasciicircum}3\$ satisfying minimal rigidity constraints (\${\textbackslash}geq 3\$ contacts per sphere and \${\textbackslash}geq 3n-6\$ total contacts). We derive such packings for \$n {\textbackslash}leq 10\$ and provide a preliminary set of maximum contact packings for \$10 {\textless} n {\textbackslash}leq 20\$. The resultant set of packings has some striking features; among them are the following: (i) all minimally rigid packings for \$n {\textbackslash}leq 9\$ have exactly \$3n-6\$ contacts; (ii) nonrigid packings satisfying minimal rigidity constraints arise for \$n {\textbackslash}geq 9\$; (iii) the number of ground states (i.e., packings with the maximum number of contacts) oscillates with respect to n; (iv) for \$10 {\textbackslash}leq n {\textbackslash}leq 20\$ there are only a small number of packings with the maximum number of contacts, and for \$10 {\textbackslash}leq n {\textless} 13\$ these are all commensurate with the hexagonal close-packed lattice. The general method presented here may have applications to other related problems in mathematics, such as the Erdös repeated distance problem and Euclidean distance matrix completion problems.},
	pages = {1860--1901},
	number = {4},
	journaltitle = {{SIAM} Journal on Discrete Mathematics},
	shortjournal = {{SIAM} J. Discrete Math.},
	author = {Arkus, N. and Manoharan, V. and Brenner, M.},
	date = {2011-01-01},
	file = {1011.5412.pdf:C\:\\Users\\Tromb\\Zotero\\storage\\5747VGWC\\1011.5412.pdf:application/pdf;Snapshot:C\:\\Users\\Tromb\\Zotero\\storage\\8QNEA9TQ\\100784424.html:text/html}
}

