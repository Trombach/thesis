	author = {Yu, Shun and Ahmadi, Sareh and Zuleta, Marcelo and Tian, Haining and Schulte, Karina and Pietzsch, Annette and Hennies, Franz and Weissenrieder, Jonas and Yang, Xichuan and Göthelid, Mats},
	date = {2010-12},
	keywords = {cyanoacryl, rutile},
	file = {Yu et al. - 2010 - Adsorption geometry, molecular interaction, and charge transfer of triphenylamine-based dye on rutile TiO2(110):C\:\\Users\\Tromb\\Zotero\\storage\\VGF4GE8N\\Yu et al. - 2010 - Adsorption geometry, molecular interaction, and charge transfer of triphenylamine-based dye on rutile TiO2(110).pdf:application/pdf}
