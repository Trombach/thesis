@article{prilutski_structure_1999-1,
  title = {Structure, Dynamics and Optical Properties of Fullerenes {{C60}}, {{C70}}},
  volume = {37},
  doi = {10.1016/S0008-6223(98)00280-2},
  abstract = {The structure, dynamics and optical properties of C60 and C70 fullerenes and their crystalline phases were studied. Their intramolecular spectra were calculated within the framework of a vibrational model using only four independent force constants for the determination of the potential energy of these molecules. The dynamic Jahn-Teller effect in C60 was also quantitatively analyzed. The equilibrium structure of C60 and C70 is determined by the minimum of the free energy function. Theoretical group analysis of the dynamics problem in the center of the Brillouin zone yields an analytical expression for 9 and 11 fundamental intermolecular frequencies for C60 and C70, respectively. The spin-lattice relaxation of both fullerenes were also studied.},
