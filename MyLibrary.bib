	shortjournal = {Biophysical Journal},
	author = {Nick Pace, C. and Martin Scholtz, J.},
	date = {1998-07-01},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Tromb\\Zotero\\storage\\ZQCVIGM6\\Nick Pace and Martin Scholtz - 1998 - A Helix Propensity Scale Based on Experimental Stu.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Tromb\\Zotero\\storage\\WHM6VSFW\\S0006349598775290.html:text/html}
