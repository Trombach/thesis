@article{schulz_structural_2001,
	title = {Structural variations and bonding in gold halides: a quantum chemical study of monomeric and dimeric gold monohalide and gold trihalide molecules, {AuX}, Au2X2, {AuX}3, and Au2X6 (X = F, Cl, Br, I).},
	volume = {7},
	doi = {10.1002/1521-3765(20010903)7:17<3657::AID-CHEM3657>3.0.CO;2-Q},
	abstract = {The molecular structures of all gold mono- and trihalides and of their dimers have been calculated at the B3LYP, {MP}2, and {CCSD}(T) levels of theory by using relativistic pseudopotentials for all atoms except fluorine. Our computations support the experimental observation that the relative stability of the monohalides increases from the fluoride toward the iodide, while the stability trend of the trihalides is the opposite. The potential energy surface ({PES}) of all gold trihalides has been investigated. These molecules are typical Jahn-Teller systems; the trigonal planar D3h-symmetry geometry does not correspond to the minimum energy structure for any of them. At the same time, the amount and character of their Jahn-Teller distortion changes gradually from {AuF}3 to {AuI}3. The minimum energy geometry is a T-shaped structure for {AuF}3 and {AuCl}3, with a Y-shaped transition-state structure. For {AuI}3, the Y-shaped structure lies lower than the T-shaped structure on the {PES}. For {AuBr}3 and {AuI}3, neither of them is the global minimum but instead an L-shaped structure, which lies outside the Jahn-Teller {PES}. This structure can be considered to be a donor-acceptor system, or a closed-shell interaction, with I2 acting as donor and {AuI} as acceptor. The dimers of gold monohalides have very short gold-gold distances and demonstrate the aurophilic interaction. The dimers of the trihalides are planar molecules with two bridging halogen atoms.},
	pages = {3657--3657},
	journaltitle = {Chemistry - A European Journal},
	author = {Schulz, A and Hargittai, M},
	date = {2001},
	keywords = {gold, distortion, halogens, hyper-, jahn, metal, metal interac-, teller, valent compounds},
	file = {Schulz_et_al-2001-Chemistry_-_A_European_Journal:C\:\\Users\\Tromb\\Zotero\\storage\\Q4PX58QJ\\Schulz_et_al-2001-Chemistry_-_A_European_Journal.pdf:application/pdf;Schulz_et_al-2001-Chemistry_-_A_European_Journal:C\:\\Users\\Tromb\\Zotero\\storage\\KCMTBW38\\Schulz_et_al-2001-Chemistry_-_A_European_Journal.pdf:application/pdf}
}

